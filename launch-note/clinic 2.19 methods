def employed(people)
 people.each do |person|
if people[0].keys.include?(:employer)
    puts people[:first_name][:last_name]
    end
    else
    end

def employers(people)
employers = Array.new
    people.each do |person|
    if person.keys.include?(:employer)
    employers << person[:employer]
    else
    end
end
