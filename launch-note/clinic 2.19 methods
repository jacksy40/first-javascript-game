methods

def is_a_fixnum?(some_input)
some_input.class ++ Fixnum
end

def is_coder?(age, proffession, language_array)
language_array.map! do |language|
language.downcase!
age == 26 && proffesion == "coder" && language_array.include?("ruby")
#returns true or false
end

